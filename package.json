{
  "name": "gatsby-skeleton-starter",
  "description": "Gatsby Skeleton Starter",
  "version": "0.0.1",
  "author": "Pedr Browne <info@undistraction.com>",
  "keywords": ["gatsby", "styled-components", "cssapi"],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "gatsby develop",
    "open": "open http://localhost:8000",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "preview": "gatsby build && gatsby serve",
    "deploy-staging":
      "yarn run build && yarn run surge ./public example-com-staging.surge.sh",
    "deploy-production":
      "yarn run build && yarn run surge ./public example.com",
    "test": "jest src/config src/styles --watch ",
    "graph": "open http://localhost:8000/___graphql",
    "lint:js": "eslint src",
    "lint:css": "stylelint './src/**/*.js'",
    "lint": "npm run lint:js && npm run lint:css",
    "demo":
      "yarn demo:import && yarn demo:copy && yarn demo:build && yarn demo:githubPages && yarn demo:open",
    "demo:import": "bash scripts/demo-import.sh",
    "demo:copy": "bash scripts/demo-copy-content.sh",
    "demo:build": "cd ./demo/project && gatsby build --prefix-paths",
    "demo:githubPages": "gh-pages -d demo/project/public",
    "demo:open": "open https://undistraction.github.io/gatsby-skeleton-starter",
    "demo:update": "yarn demo:import && yarn demo:dev",
    "demo:deleteCache": "bash scripts/demo-delete-cache.sh",
    "demo:dev":
      "yarn demo:deleteCache && yarn demo:copy && cd demo/project && yarn dev",
    "demo:serve": "cd ./demo/project && gatsby serve",
    "surge:deploy": "surge --domain gatsby-skeleton-starter.surge.sh public/"
  },
  "dependencies": {
    "cssapi": "0.8.8",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.7.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.2.6",
    "styled-normalize": "^4.0.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.5.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "gatsby": "^1.9.259",
    "gatsby-image": "^1.0.14",
    "gatsby-link": "^1.6.21",
    "gatsby-paginate": "^1.0.6",
    "gatsby-plugin-accessibilityjs": "^1.0.3",
    "gatsby-plugin-catch-links": "^1.0.9",
    "gatsby-plugin-favicon": "^2.0.0",
    "gatsby-plugin-feed": "^1.3.15",
    "gatsby-plugin-google-analytics": "^1.0.8",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-plugin-remove-trailing-slashes": "^1.0.9",
    "gatsby-plugin-robots-txt": "^1.0.2",
    "gatsby-plugin-sharp": "^1.6.43",
    "gatsby-plugin-sitemap": "^1.2.23",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-plugin-twitter": "^1.0.13",
    "gatsby-remark-code-repls": "^1.0.10",
    "gatsby-remark-copy-linked-files": "^1.5.32",
    "gatsby-remark-embed-snippet": "^1.0.2",
    "gatsby-remark-embed-video": "^1.2.0",
    "gatsby-remark-embedded-codesandbox": "^1.1.0",
    "gatsby-remark-images": "^1.5.62",
    "gatsby-remark-prismjs": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^1.4.18",
    "gatsby-remark-responsive-image": "^1.0.0-alpha13-alpha.435e0178",
    "gatsby-remark-smartypants": "^1.4.12",
    "gatsby-source-filesystem": "^1.5.5",
    "gatsby-transformer-remark": "^1.7.40",
    "gatsby-transformer-sharp": "^1.6.12",
    "gh-pages": "^1.1.0",
    "github-pages": "^3.0.2",
    "jest": "^21.2.1",
    "postcss-sass": "^0.2.0",
    "prettier": "^1.7.4",
    "rehype-react": "^3.0.2",
    "remark-retext": "^3.1.0",
    "retext-english": "^3.0.0",
    "slugify": "^1.2.1",
    "stylelint": "^8.3.1",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.2.0",
    "surge": "^0.19.0"
  },
  "browserslist": ["> 1%", "IE >= 9", "last 2 versions"]
}
